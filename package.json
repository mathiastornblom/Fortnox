{
    "name": "fortnox-wrapper",
    "version": "1.0.2",
    "description": "Unofficial Fortnox api-wrapper for customers, articles and invoices. built from https://github.com/obrut/fortnox",
    "main": "./dist/index.js",
    "typings": "./dist/index",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "auth": "./node_modules/.bin/ts-node ./test/http-server-oauth-authorize.ts",
        "test": "./node_modules/.bin/nyc ./node_modules/.bin/mocha -r ts-node/register test/*.spec.ts"
    },
    "author": "Mathias TÃ¶rnblom",
    "repository": "github:mathiastornblom/fortnox",
    "keywords": [
        "fortnox",
        "api",
        "node"
    ],
    "bugs": "https://github.com/mathiastornblom/fortnox/issues",
    "license": "MIT",
    "devDependencies": {
        "@types/chai": "^4.3.4",
        "@types/mocha": "^10.0.1",
        "@types/node-fetch": "^2.6.2",
        "chai": "^4.3.7",
        "mocha": "^10.1.0",
        "nyc": "^15.1.0",
        "register": "0.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.3"
    },
    "dependencies": {
        "node-fetch": "^2.6.1"
    },
    "nyc": {
        "extension": [
            ".ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "include": [
            "src/**/*"
        ],
        "exclude": [
            "**/*.d.ts",
            "src/dispatch.ts",
            "src/utils.ts"
        ],
        "reporter": [
            "text"
        ],
        "all": true,
        "check-coverage": true
    }
}
