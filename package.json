{
  "name": "fortnox-wrapper",
  "version": "0.1.6",
  "description": "Unofficial Fortnox api-wrapper for customers, articles and invoices.",
  "main": "./dist/index.js",
  "typings": "./dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "./node_modules/.bin/nyc ./node_modules/.bin/mocha -r ts-node/register test/*.ts"
  },
  "author": "Gustaf Ridderstolpe, Enfo Sweden AB",
  "license": "Apache",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/node-fetch": "^2.3.7",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "register": "0.0.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "node-fetch": "^2.6.0"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "text"
    ],
    "all": true,
    "check-coverage": true
  }
}
